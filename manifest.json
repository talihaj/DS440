{
  "name": "Drowsiness.tv",
  "description": "Drowsiness detection to pause video while using streaming services",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "alarms",
    "notifications",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Drowsiness Detection",
    "default_popup": "popup.html",
    "default_icon": "drowsniess.jpeg"
  }
}
